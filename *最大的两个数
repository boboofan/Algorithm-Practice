#include<iostream>
#include<algorithm>
using namespace std;

struct Anode{
	int n,s;
}a[4][5];

void Print()
{
	for(int j=0;j<5;j++)
	{
		for(int i=0;i<4;i++)
		{
			if(a[i][j].s==1||a[i][j].s==2)
			{
				cout<<a[i][j].n;
				a[i][j].s=0;
				break;
			}
		}

		if(j==4)
			cout<<endl;
		else
			cout<<" ";
	}

}

int main()
{
	int i,j,t[4],tag;

	for(i=0;i<4;i++)
	{	
		for(j=0;j<5;j++)
		{
			cin>>a[i][j].n;
			a[i][j].s=0;
		}
	}
	
	for(j=0;j<5;j++)
	{
		tag=1;

		for(i=0;i<4;i++)
			t[i]=a[i][j].n;

		sort(t,t+4);

		for(i=0;i<4;i++)
		{
			if(tag==3)
				break;
			if(a[i][j].n==t[3])
				a[i][j].s=tag++;
		}

		for(i=0;i<4;i++)
		{
			if(tag==3)
				break;
			if(a[i][j].n==t[2])
				a[i][j].s=tag++;
		}
	}

	Print();
	Print();

	return 0;
}
