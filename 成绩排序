#include<iostream>
#include<algorithm>
using namespace std;

typedef struct Node{
	int p,q;
};

bool Cmp(struct Node a,struct Node b)
{
	if(a.q!=b.q)
		return a.q<b.q;
	else
		return a.p<b.p;
}

int main()
{
	int i,N,p,q;
	struct Node n[100];
	while(cin>>N)
	{
		for(i=0;i<N;i++)
			cin>>n[i].p>>n[i].q;

		sort(n,n+N,Cmp);

		for(i=0;i<N;i++)
			cout<<n[i].p<<" "<<n[i].q<<endl;
	}
	return 0;
}
