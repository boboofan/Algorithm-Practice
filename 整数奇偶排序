#include<iostream>
#include<algorithm>
using namespace std;

bool Cmp(int a,int b)
{
	if(a%2==1&&b%2==1)
		return a>b;
	else if(a%2==1&&b%2==0)
		return true;
	else if(a%2==0&&b%2==1)
		return false;
	else
		return a<b;
}

int main()
{
	int i,a[10];
	while(cin>>a[0]>>a[1]>>a[2]>>a[3]>>a[4]>>a[5]>>a[6]>>a[7]>>a[8]>>a[9])
	{
		sort(a,a+10,Cmp);
		for(i=0;i<10;i++)
		{	
			if(i==9)
				cout<<a[i]<<endl;
			else
				cout<<a[i]<<" ";
		}
	}
	return 0;
}
