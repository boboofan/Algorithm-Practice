#include<iostream>
#include<stack>
using namespace std;

int main()
{
	int n,N;
	char c;
	stack<int>s;

	while(cin>>N)
	{
		while(N--)
		{
			cin>>c;

			if(c=='P')
			{
				cin>>n;
				s.push(n);
			}

			if(c=='O'&&!s.empty())
				s.pop();

			if(c=='A')
			{
				if(s.empty())
					cout<<"E"<<endl;
				else
					cout<<s.top()<<endl;
			}
		}

		while(!s.empty())
			s.pop();

		cout<<endl;
	}

	return 0;
}
