#include<iostream>
using namespace std;

int Judge(int N,int K)
{
	int year,wage,price;
	for(year=1,wage=N,price=200;year<=21;year++)
	{
		if(wage>=price)
			return year;
		wage+=N;
		price*=(1+0.01*K);
	}
	return 0;
}

int main()
{
	int N,K;
	while(cin>>N>>K)
	{
		if(Judge(N,K))
			cout<<Judge(N,K)<<endl;
		else
			cout<<"Impossible"<<endl;
	}
	return 0;
}
