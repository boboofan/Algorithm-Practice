#include<iostream>
using namespace std;

int Up(int n)
{
	if(n%2==1)
		n-=1;
	return n/2;
}

int main()
{
	int x,y;
	while(cin>>x>>y)
	{
		while(x!=y)
		{
			if(x>y)
				x=Up(x);
			else
				y=Up(y);
		}
		cout<<x<<endl;
	}
	return 0;
}
