#include<iostream>
#include<string>
#include<math.h>
using namespace std;

int Tran(char c)
{
	if(48<=c&&c<=57)
		return c-48;
	else
		return c-55;
}

int main()
{
	long i,n,t;
	string s;
	while(cin>>s)
	{
		for(n=t=0,i=s.size()-1;i>1;i--)
			n+=pow(16,t++)*Tran(s[i]);
		cout<<n<<endl;
	}
	return 0;
}
