#include<iostream>
#include<string>
using namespace std;

bool Judge(string s)
{
	for(int i=0;i<s.size();i++)
		if(s[i]!=s[s.size()-i-1])
			return false;
	return true;
}

int main()
{
	string s;
	while(cin>>s)
	{	
		if(Judge(s))
			cout<<"Yes!"<<endl;
		else
			cout<<"No!"<<endl;
	}
	return 0;
}
